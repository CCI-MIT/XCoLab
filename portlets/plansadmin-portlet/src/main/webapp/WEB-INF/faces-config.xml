<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!-- application related managed beans -->
	<!--
	<managed-bean>
		<managed-bean-name>discussionPrefs</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.discussions.DiscussionPrefs</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
    -->
	<application>
		<!--
		<message-bundle>messages-override</message-bundle>
		<resource-bundle>
			<base-name>Language</base-name>
			<var>msgs</var>
		</resource-bundle>
        -->
		<view-handler>com.icesoft.faces.facelets.D2DFaceletViewHandler</view-handler>
	</application>
	<factory/>
	<managed-bean>
		<managed-bean-name>edit</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.PortletPreferencesManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>language</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.LanguageManagedBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>renderManager</managed-bean-name>
		<managed-bean-class>com.icesoft.faces.async.render.RenderManager</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resumeUploader</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.icefaces.FileUploadManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>renderManager</property-name>
			<value>#{renderManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>themeDisplay</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.ThemeDisplayManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>plansAdmin</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.plansadmin.PlansAdminBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>contestsAdmin</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.plansadmin.ContestsAdmin</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>planSectionDefinitionsAdmin</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.plansadmin.PlanSectionDefinitionsAdmin</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>planTemplatesAdmin</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.plansadmin.PlanTemplatesAdmin</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>contestPhaseTypeBean</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.plansadmin.ContestPhaseTypeBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/xhtml/view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editContest</from-outcome>
			<to-view-id>/xhtml/editContest.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/xhtml/view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editPlanSectionDefinition</from-outcome>
			<to-view-id>/xhtml/editPlanSectionDefinition.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/xhtml/view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editPlanTemplate</from-outcome>
			<to-view-id>/xhtml/editPlanTemplate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/xhtml/*</from-view-id>
		<navigation-case>
			<from-outcome>backToIndex</from-outcome>
			<to-view-id>/xhtml/view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>