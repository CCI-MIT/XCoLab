<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN"
        "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">
<routes>

    <route>
        <pattern>/createContest</pattern>
        <generated-parameter name="createContest">true</generated-parameter>
    </route>

    <route>
        <pattern>/create2015Tier1Contests</pattern>
        <generated-parameter name="create2015Tier1Contests">true</generated-parameter>
    </route>

    <route>
        <pattern>/create2015Tier2And3Contests</pattern>
        <generated-parameter name="create2015Tier2And3Contests">true</generated-parameter>
    </route>

    <route>
        <pattern>/contestId/{contestId}</pattern>
        <generated-parameter name="tab">DESCRIPTION</generated-parameter>
        <generated-parameter name="contestId">{contestId}</generated-parameter>
    </route>

    <route>
        <pattern>/contestId/{contestId}/tab/{tab}</pattern>
        <generated-parameter name="tab">{tab}</generated-parameter>
        <generated-parameter name="contestId">{contestId}</generated-parameter>
    </route>

    <route>
        <pattern>/manager</pattern>
        <generated-parameter name="tab">OVERVIEW</generated-parameter>
        <generated-parameter name="manager">true</generated-parameter>
    </route>

    <route>
        <pattern>/manager/tab/{tab}</pattern>
        <generated-parameter name="tab">{tab}</generated-parameter>
        <generated-parameter name="manager">true</generated-parameter>
    </route>

    <route>
        <pattern>/manager/tab/{tab}/elementId/{elementId}</pattern>
        <generated-parameter name="tab">{tab}</generated-parameter>
        <generated-parameter name="elementId">{elementId}</generated-parameter>
        <generated-parameter name="manager">true</generated-parameter>
    </route>


</routes>