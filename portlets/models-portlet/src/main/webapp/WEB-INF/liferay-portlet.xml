<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>models-portlet</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<requires-namespaced-parameters>false</requires-namespaced-parameters> 
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/xmlhttp/css/simulation/simulation.css</header-portlet-css>
		<footer-portlet-javascript>/../html/js/jquery/jquery.jqplot.js</footer-portlet-javascript>
		<footer-portlet-javascript>/../html/js/jquery/jqplot.ohlcRenderer.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/../html/js/jquery/jqplot.barRenderer.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/../html/js/jquery/jqplot.canvasTextRenderer.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/../html/js/jquery/jqplot.canvasAxisLabelRenderer.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/../html/js/editor/ckeditor/ckeditor.js</footer-portlet-javascript>
		<footer-portlet-javascript>/xmlhttp/js/navigation/navigationManager.js</footer-portlet-javascript>
		<footer-portlet-javascript>/xmlhttp/js/simulation/model-inputs-render.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>
