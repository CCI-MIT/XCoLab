<jsp:root 
	xmlns:jsp="http://java.sun.com/JSP/Page" 
	xmlns:portlet="http://java.sun.com/portlet_2_0"
	xmlns:liferay-theme="http://liferay.com/tld/theme" version="2.0">
	<portlet:defineObjects />
	<liferay-theme:defineObjects />

    <jsp:useBean id="contestType" scope="request" type="org.xcolab.client.contest.pojo.ContestType" />
    <jsp:useBean id="colabName" scope="request" type="java.lang.String" />
    <jsp:useBean id="colabShortName" scope="request" type="java.lang.String" />
</jsp:root>