<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:collab="http://climatecolab.org/tags/collab_1.0"
	version="2.0">

	<jsp:directive.attribute name="threadId" type="java.lang.Long" required="true" rtexprvalue="true" description="Thread id" />
	<jsp:directive.attribute name="sharedColabId" type="java.lang.Long" required="false" rtexprvalue="true" description="ContestId id" />
	<collab:discussionComments threadId="${threadId}" sharedColabId="${sharedColabId}"/>
</jsp:root>