<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN"
"http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">
<routes>
    <route>
        <pattern>/page/{pageTitle}</pattern>
        <generated-parameter name="pageTitle">{pageTitle}</generated-parameter>
        <implicit-parameter name="show">wiki</implicit-parameter>
    </route>

    <route>
        <pattern>/resources/{contestYear}/{contestUrlName}</pattern>
        <generated-parameter name="contestUrlName">{contestUrlName}</generated-parameter>
        <generated-parameter name="contestYear">{contestYear}</generated-parameter>
        <implicit-parameter name="show">resource</implicit-parameter>
    </route>
</routes>