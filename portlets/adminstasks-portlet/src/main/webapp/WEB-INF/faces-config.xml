<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>adminTask</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.admintasks.AdminTasksBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>fixForRemovedProposalsTeamMembers</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.admintasks.FixForRemovedProposalsTeamMembers</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>fixCommentActivities</managed-bean-name>
		<managed-bean-class>org.xcolab.portlets.admintasks.FixCommentActivities</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
    <managed-bean>
        <managed-bean-name>batchRegisterUsers</managed-bean-name>
        <managed-bean-class>org.xcolab.portlets.admintasks.BatchRegisterUsers</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>