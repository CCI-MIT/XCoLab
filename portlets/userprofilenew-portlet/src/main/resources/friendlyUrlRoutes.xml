<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN"
"http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">
<routes>
    <route>
        <pattern>/userId/{userId}</pattern>
        <generated-parameter name="userId">{userId}</generated-parameter>
    </route>
    <route>
        <pattern>/userId/{userId}/page/edit</pattern>
        <generated-parameter name="userId">{userId}</generated-parameter>
        <generated-parameter name="edit">true</generated-parameter>
    </route>

    <route>
        <pattern>/userId/{userId}/page/subscriptionsManage</pattern>
        <generated-parameter name="userId">{userId}</generated-parameter>
        <generated-parameter name="subscriptionsManage">true</generated-parameter>
    </route>


</routes>