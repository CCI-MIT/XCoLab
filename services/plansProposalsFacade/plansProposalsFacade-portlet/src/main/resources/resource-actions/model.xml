<!-- Resource action mappings for plans portlet -->
<resource-action-mapping>
	<portlet-resource>
		<portlet-name>models</portlet-name>
		<supports>
			<action-key>ADD_POSITION</action-key>
			<action-key>ADD_PLAN</action-key>
			<action-key>CONFIGURATION</action-key>
			<action-key>VIEW</action-key>
		</supports>
		<community-defaults>
			<action-key>VIEW</action-key>
			<action-key>ADD_PLAN</action-key>
		</community-defaults>
		<guest-defaults>
			<action-key>VIEW</action-key>
		</guest-defaults>
		<guest-unsupported>
			<action-key>ADD_POSITION</action-key>
			<action-key>ADD_PLAN</action-key>
			<action-key>CONFIGURATION</action-key>
		</guest-unsupported>
	</portlet-resource>
</resource-action-mapping>