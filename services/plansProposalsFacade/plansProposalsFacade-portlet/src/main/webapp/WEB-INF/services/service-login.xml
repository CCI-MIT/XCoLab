<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_2_0.dtd">

<service-builder package-path="com.ext.portlet.LoginLog">
    <entity name="LoginLog" local-service="true" >
        <column name="pk" type="long" primary="true" />
        <column name="userId" type="long" />
        <column name="createDate" type="Date" />
        <column name="ipAddress" type="String" />
        <column name="city" type="String" />
        <column name="country" type="String" />
        <column name="entryUrl" type="String" />

    </entity>
    <exceptions>
        <exception>NoSuchLoginLogException</exception>
    </exceptions>
</service-builder>