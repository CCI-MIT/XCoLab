<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_2_0.dtd">

<service-builder package-path="com.ext.portlet.xcolab">
    <entity name="ConfigurationAttribute" local-service="true">
        <column name="name" type="String" primary="true" />
        <column name="additionalId" type="long" primary="true" />
        <column name="numericValue" type="long" />
        <column name="stringValue" type="String" />
        <column name="realValue" type="double" />
    </entity>

</service-builder>