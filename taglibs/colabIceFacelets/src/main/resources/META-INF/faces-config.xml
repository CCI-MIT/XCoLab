<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!--REMOVE_FROM_SHARED_LIB_BEGIN-->
	<!-- REMOVE_FROM_SHARED_LIB_END  -->
	<!--
	<lifecycle>
		<phase-listener>org.climatecollaboratorium.navigation.NavigationPhaseListener</phase-listener>
	</lifecycle>
     -->
	<managed-bean>
		<managed-bean-name>edit</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.PortletPreferencesManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>language</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.LanguageManagedBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>renderManager</managed-bean-name>
		<managed-bean-class>com.icesoft.faces.async.render.RenderManager</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resumeUploader</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.icefaces.FileUploadManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>renderManager</property-name>
			<value>#{renderManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>themeDisplay</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.ThemeDisplayManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>simulationBeanTest</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.facelets.simulations.test.SimulationTestBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>discussionTestBean</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.facelets.discussions.test.DiscussionTestBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>discussionBean</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.facelets.discussions.DiscussionBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>eventBus</property-name>
			<value>#{eventBus}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>eventBus</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.events.EventBusImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>simulationBean</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.facelets.simulations.SimulationBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>eventBus</property-name>
			<value>#{eventBus}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>navigationManagerBean</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.navigation.NavigationManagerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>eventBus</property-name>
			<value>#{eventBus}</value>
		</managed-property>
		<managed-property>
			<property-name>sendUnchanged</property-name>
			<value>#{true}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>navigationManagerBeanTest</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.navigation.test.NavigationManagerBeanTest</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>eventBus</property-name>
			<value>#{eventBus}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tagsDemo</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.tagsdemo.TagsDemo</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>eventBus</property-name>
			<value>#{eventBus}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>migrationTool</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.utils.MigrationTool</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>simulationTester</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.utils.SimulationTester</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>plansTool</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.utils.PlansTool</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<validator>
		<validator-id>requiredFieldsValidator</validator-id>
		<validator-class>org.climatecollaboratorium.validators.RequiredFieldValidator</validator-class>
	</validator>
	<validator>
		<validator-id>discussionCategoryNameUniqueValidator</validator-id>
		<validator-class>org.climatecollaboratorium.facelets.discussions.validators.CategoryNameValidator</validator-class>
	</validator>
</faces-config>