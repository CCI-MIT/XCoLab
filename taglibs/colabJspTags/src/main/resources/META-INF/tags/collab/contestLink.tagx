<jsp:root 
  xmlns:c="http://java.sun.com/jsp/jstl/core" 
  xmlns:jsp="http://java.sun.com/JSP/Page"  
  version="2.0">
  <jsp:directive.attribute name="contestId" type="java.lang.Long" required="true" rtexprvalue="true" description="Contest id" />
  <jsp:directive.attribute name="text" type="java.lang.String" required="true" rtexprvalue="true" description="Link text" />
  <jsp:directive.attribute name="linkId" type="java.lang.String" required="false" rtexprvalue="true" description="HTML id" />
  <jsp:directive.attribute name="target" type="java.lang.String" required="false" rtexprvalue="true" description="HTML target value" />
  <a href="/web/guest/plans/-/plans/contestId/${contestId}" id="${linkId}" target="${target}">${text}</a>
</jsp:root>